{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_adls",
						"type": "DatasetReference"
					},
					"name": "adlssourceerrorhandle"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "split1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          empid as short,",
				"          empname as string,",
				"          empsal as short,",
				"          depid as short,",
				"          gender as string,",
				"          dob as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> adlssourceerrorhandle",
				"adlssourceerrorhandle split(isNull(toDate(dob,'yyyy-MMM-dd')),",
				"     disjoint: false) ~> split1@(errorrows, goodrows)"
			]
		}
	}
}