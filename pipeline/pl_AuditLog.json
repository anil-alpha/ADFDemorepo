										},
										"RunID": {
											"value": "'@{pipeline().RunId}'",
											"type": "Expression"
										},
										"TrigerName": {
											"value": "'@{pipeline().TriggerName}'",
											"type": "Expression"
										},
										"LogFileName": {
											"value": "'@{variables('LogFileName')}'",
											"type": "Expression"
										}
									}
								},
								"compute": {
									"coreCount": 8,
									"computeType": "General"
								},
								"traceLevel": "Fine"
