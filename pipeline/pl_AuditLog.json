										},
										"Status": {
											"value": "'@{string('success')}'",
											"type": "Expression"
										},
										"RunID": {
											"value": "'@{pipeline().RunId}'",
											"type": "Expression"
										},
										"TrigerName": {
											"value": "'@{pipeline().TriggerName}'",
											"type": "Expression"
										},
										"LogFileName": {
											"value": "'@{variables('LogFileName')}'",
											"type": "Expression"
										}
									},
									"datasetParameters": {
										"Log": {
											"LogFileName": {
												"value": "@variables('LogFileName')",
												"type": "Expression"
											}
